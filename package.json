{
  "name": "js13kgame",
  "version": "0.1.0",
  "description": "Glitch or die trying",
  "main": "src/main.js",
  "scripts": {
    "dev": "gulp watch",
    "build": "gulp build --uglify",
    "build-debug": "gulp --uglify --sourcemaps",
    "build-google": "gulp build --googlecompiler",
    "build-google-debug": "gulp build --googlecompiler --sourcemaps",
    "release": "gulp release --uglify",
    "deploy": "npm run release && gh-pages -d ./dist -m \"Size: $(find ./dist -maxdepth 1 -printf '%s\t%f\n' | grep game.gz | grep -Po '[0-9]+')\"",
    "test": "rollup -i src/mainSpec.js -o dist/bundle-tests.js && babel dist/bundle-tests.js --out-file dist/bundle-tests-es5.js",
    "dev.old": "watch 'npm run test && npm run devjs' ./src",
    "devjs.old": "rollup -c -o dist/bundle.js && babel dist/bundle.js | indexhtmlify --title js13kgame --style src/main.css > dist/index.html",
    "build.old": "rollup -c -o dist/bundle.js && babel dist/bundle.js | uglifyjs -cm | indexhtmlify --title js13kgame --style src/main.css | html-minifier --minify-css > dist/index.html",
    "release.old": "gzip -9 -c dist/index.html > dist/game.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-thirtyfour/js13kgame.git"
  },
  "author": "team-thirtyfour",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/team-thirtyfour/js13kgame/issues"
  },
  "homepage": "https://github.com/team-thirtyfour/js13kgame#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "gh-pages": "^1.2.0",
    "google-closure-compiler": "^20180805.0.0",
    "gulp": "3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-connect": "^5.5.0",
    "gulp-gzip": "^1.4.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-inject": "^4.3.2",
    "gulp-jasmine": "^4.0.0",
    "gulp-jshint": "2.1.0",
    "gulp-rename": "^1.4.0",
    "gulp-sequence": "^1.0.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-uglify": "3.0.1",
    "gulp-util": "^3.0.8",
    "html-minifier": "3.5.19",
    "indexhtmlify": "1.3.1",
    "jasmine-reporters": "^2.3.2",
    "jshint": "^2.9.6",
    "jshint-stylish": "^2.2.1",
    "require-dir": "^1.0.0",
    "rollup": "0.64.1",
    "rollup-stream": "^1.24.1",
    "rollup-watch": "4.3.1",
    "uglify-js": "3.4.7",
    "vinyl-source-stream": "^2.0.0",
    "watch": "1.0.2"
  }
}
